{% block layout_css %}
    <!--=== CSS ===-->
    {% if minified_css != false %}
        <!--=== Minified ===-->
        <link href="{{ minified_css }}" rel="stylesheet" type="text/css" />
    {% else %}
        <!-- CoreUI CSS -->
        <link rel="stylesheet" href="https://unpkg.com/@coreui/coreui@3.0.0-beta.1/dist/css/coreui.min.css" crossorigin="anonymous">
    {% endif %}
{% endblock layout_css %}
